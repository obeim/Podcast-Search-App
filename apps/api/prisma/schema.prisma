generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Podcast {
  trackId      Int           @id
  trackName    String
  artistName   String
  artworkUrl   String
  searchTerms  SearchTerm[]  
}

model SearchTerm {
  id       Int        @id @default(autoincrement())
  term     String     @unique
  podcasts Podcast[]  
}