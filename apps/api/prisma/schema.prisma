generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Podcast {
  id          Int     @id @default(autoincrement())
  trackId     Int     @unique
  trackName   String
  artistName  String
  artworkUrl  String?
  feedUrl     String?
  searchedTerm        String?
  createdAt   DateTime @default(now())
}